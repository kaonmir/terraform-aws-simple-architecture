{
  "aws_region": "ap-northeast-2",
  "number_of_subnet": 2,
  "project_name": "semo",

  "launch_template": {
    "ec2_type": "t3.small",
    "ebs_type": "gp2",
    "ebs_volume_size": 15,
    "extra_user_data": "",
    "update_default_version": true
  },

  "asg": {
    "desired_capacity": 2,
    "max_size": 4,
    "min_size": 2,
    "extra_user_data": ""
  },

  "alb_deletion_protection": false,

  "ecr": {
    "registry": "",
    "repository": "nginx",
    "tag": "latest"
  },

  "app": {
    "port": 80,
    "allow_http_access": true,
    "allow_https_access": false,
    "certificate_arn": ""
  }
}
